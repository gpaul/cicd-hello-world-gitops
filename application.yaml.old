kind: Deployment
metadata:
  name: hello-world
spec:
  selector:
    matchLabels:
      app: hello-world
  template:
    metadata:
      labels:
        app: hello-world
    spec:
      containers:
      - image: "gpaul/hello-world@sha256:561f7b0510f59a88342d54c15945ae92189f078f8c7a1a2e3a15802015815361"
        name: hello-world
        ports:
        - containerPort: 8080
